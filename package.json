{
    "name": "myapp",
    "version": "0.1.0",
    "description": "My app.",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "babel-build": "babel src --out-dir dist --source-maps",
        "build": "webpack --config webpack.config.js --mode development",
        "build-dev": "webpack --config webpack.config.js --mode development",
        "build-prod": "webpack --config webpack.config.js --mode production",
        "watch": "npx webpack serve --open --port 80 --mode development",
        "watch:js": "onchange \"src/**/*.js\" \"src/**/*.jsx\" \"src/index.html\" -- npm run build-prod",
        "merge-package-json": "cp package.json package-main.json && package-json-merge package-main.json package-overrides.json > package.json",
        "backup-package-json": "cp package.json package-orig.json",
        "reset-package-json": "cp package-orig.json package.json",
        "start": "webpack serve",
        "start-server": "nodemon app.js",
        "lint": "eslint src/**",
        "lint-fix": "eslint --fix src/**",
        "git-reset": "git reset --soft HEAD~1",
        "git-force": "git push origin +main --force"
    },
    "author": "Jos√© Bernal",
    "license": "ISC",
